<%= form_with(model: @house, local: true) do |form| %>

  <%= form.label :house_name, '物件名' %>
  <%= form.text_field :house_name %>
  <%= form.label :price, '賃料' %>
  <%= form.text_field :price %>
  <%= form.label :address,'住所' %>
  <%= form.text_field :address %>
  <%= form.label :age, '築年数' %>
  <%= form.text_field :age %>
  <%= form.label :note, '備考' %>
  <%= form.text_area :note %>

  <%= form.submit '登録' %>
<% end %>
